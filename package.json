    "@types/bcryptjs": "^2.4.6",
    "apexcharts": "^4.4.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "dropzone": "^6.0.0-beta.2",
    "express": "^5.1.0",
    "flatpickr": "^4.6.13",
    "helmet": "^8.1.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "jsvectormap": "^1.6.0",
    "lucide-vue-next": "^0.474.0",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.1",
