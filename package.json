{
  "name": "tailadmin-vue-pro-2.0.1",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:tools": "vite --mode development",
    "dev:prod": "vite --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --project tsconfig.build.json",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "seed": "tsx scripts/seed-database.ts",
    "mcp-server": "tsx scripts/start-mcp-server.ts",
    "mcp-atlas": "tsx scripts/start-atlas-mcp.ts",
    "dev:with-mcp": "concurrently \"npm run mcp-server\" \"npm run dev\"",
    "dev:with-atlas": "concurrently \"npm run mcp-atlas\" \"npm run dev\"",
    "start": "node server/working-server.cjs",
    "start:all": "concurrently \"node server/working-server.cjs\" \"npm run dev\"",
    "start:dev": "node start-servers.js",
    "setup-atlas": "node scripts/setup-atlas-credentials.js",
    "server": "tsx server/index.ts",
    "server:dev": "tsx --watch server/index.ts",
    "seed:users": "tsx server/scripts/seed-users.ts",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "vercel-build": "npm run build-only"
  },
  "dependencies": {
